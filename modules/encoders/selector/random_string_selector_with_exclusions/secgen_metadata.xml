<?xml version="1.0"?>

<encoder xmlns="http://www.github/cliffe/SecGen/encoder"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.github/cliffe/SecGen/encoder">
  <name>Random String Selector with Exclusions</name>
  <author>Thomas Shaw</author>
  <module_license>MIT</module_license>
  <description>Randomly selects one of the inputs and discards the rest. Will not select any values found in the
    exclusion list.
  </description>

  <type>string_selector_with_exclusions</type>
  <platform>linux</platform>
  <platform>windows</platform>

  <read_fact>strings_to_encode</read_fact>
  <read_fact>exclusion_list</read_fact>

  <output_type>selected_string</output_type>
</encoder>

<?xml version='1.0' encoding='UTF-8' ?>

<pounces version='1.0'>
	<pounce ui='gtk-gaim'>
		<account protocol='prpl-irc'><%=@username-%>@<%=@ip%></account>
		<pouncee>Hackerbot</pouncee>
		<options/>
		<events>
			<event type='sign-on'/>
		</events>
		<actions>
			<action type='send-message'>
				<param name='message'>hello</param>
			</action>
			<action type='open-window'/>
		</actions>
		<save/>
	</pounce>
</pounces>
